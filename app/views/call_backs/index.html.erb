<h1>Listing call_backs</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Repository</th>
    <th>Url</th>
    <th>Payload</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @call_backs.each do |call_back| %>
  <tr>
    <td><%= call_back.username %></td>
    <td><%= call_back.repository %></td>
    <td><%= call_back.url %></td>
    <td><%= call_back.payload %></td>
    <td><%= link_to 'Show', call_back %></td>
    <td><%= link_to 'Edit', edit_call_back_path(call_back) %></td>
    <td><%= link_to 'Destroy', call_back, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Call back', new_call_back_path %>
